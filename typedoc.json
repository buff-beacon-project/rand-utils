{
  "visibilityFilters": {
    "protected": false,
    "private": false,
    "inherited": true,
    "external": false,
    "@alpha": false,
    "@beta": true
  },
  "excludePrivate": true,
  "validation": {
    "notExported": true,
    "invalidLink": true,
    "notDocumented": true
  },
  "includeVersion": true,
  "readme": "none",
  "entryFileName": "README.md",
  "out": "./docs/",
  "exclude": ["*.test.ts"],
  "plugin": ["typedoc-plugin-markdown"],
  "indexFormat": "table"
}
