{
  "name": "rand-utils",
  "version": "0.0.1",
  "description": "Tools for consuming public randomness sources",
  "scripts": {
    "build": "webpack --mode=production && npm run test",
    "dev": "webpack --progress --watch --mode=production",
    "run-test": "npm run build && node test/run.js",
    "test": "BABEL_ENV=test mocha --require @babel/polyfill --require @babel/register --colors ./test/*.spec.mjs",
    "test:watch": "mocha --require babel-register --colors -w ./test/*.spec.mjs",
    "docs": "documentation build src/** -f md --github -o API.md"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "keywords": [],
  "author": "Jasper Palfree",
  "license": "MIT",
  "bugs": {
    "url": ""
  },
  "type": "module",
  "files": [
    "dist/**/*",
    "src/**/*"
  ],
  "module": "src/index.js",
  "main": "src/index.js",
  "exports": {
    "umd": "./dist/rand-utils.node.js",
    "default": "./src/index.js"
  },
  "engines": {
    "node": ">=12"
  },
  "browserslist": [
    "last 1 version",
    "> 1%"
  ],
  "homepage": "https://random.colorado.edu",
  "devDependencies": {
    "@babel/cli": "^7.1.5",
    "@babel/core": "^7.1.6",
    "@babel/polyfill": "^7.0.0",
    "@babel/preset-env": "^7.1.6",
    "@babel/register": "^7.0.0",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.0-beta.4",
    "babel-plugin-add-module-exports": "^1.0.0",
    "babel-plugin-module-resolver": "^4.1.0",
    "chai": "^4.3.4",
    "documentation": "^13.2.5",
    "eslint": "^7.31.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.1.0",
    "eslint-plugin-standard": "^5.0.0",
    "eslint-webpack-plugin": "^3.0.1",
    "fast-copy": "^2.1.1",
    "lodash": "^4.17.21",
    "mocha": "^9.0.3",
    "terser-webpack-plugin": "^5.1.4",
    "uglifyjs-webpack-plugin": "^2.0.1",
    "webpack": "^5.45.1",
    "webpack-cli": "^4.7.2"
  },
  "dependencies": {
    "bit-buffer": "^0.2.5"
  }
}
